{
    "name": "nepada/presenter-mapping",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Improved presenter mapping for PresenterFactory from Nette.",
    "keywords": ["nette", "presenter", "mapping"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/presenter-mapping/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.4",
        "nette/utils": "^3.2@dev || ^4.0@dev",
        "nette/application": "^3.1.4@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.1",
        "phpstan/phpstan": "1.10.32",
        "phpstan/phpstan-strict-rules": "1.5.1",
        "phpstan/phpstan-nette": "1.2.9",
        "nepada/phpstan-nette-tester": "1.1.0",
        "spaze/phpstan-disallowed-calls": "2.16.0",
        "shipmonk/phpstan-rules": "2.7.0",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.13.0",
        "nette/di": "^3.0.6@dev",
        "nette/bootstrap": ">=3.1@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\PresenterMapping\\": "src/PresenterMapping/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
