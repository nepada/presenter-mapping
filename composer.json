{
    "name": "nepada/presenter-mapping",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Improved presenter mapping for PresenterFactory from Nette.",
    "keywords": ["nette", "presenter", "mapping"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/presenter-mapping/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "nette/utils": "^3.0@dev",
        "nette/application": "^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "^2.2",
        "xificurk/phpstan-shim": "^0.11.2",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "nepada/coding-standard": "^6.2.0",

        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.0-RC5@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\PresenterMapping\\": "src/PresenterMapping/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
