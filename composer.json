{
    "name": "nepada/presenter-mapping",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Improved presenter mapping for PresenterFactory from Nette.",
    "keywords": ["nette", "presenter", "mapping"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/presenter-mapping/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nette/utils": "^3.1.3@dev",
        "nette/application": "^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.1",
        "phpstan/phpstan": "0.12.94",
        "phpstan/phpstan-strict-rules": "0.12.10",
        "pepakriz/phpstan-exception-rules": "0.11.7",
        "phpstan/phpstan-nette": "0.12.21",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.9.0",
        "php-parallel-lint/php-parallel-lint": "1.3.1",
        "nepada/coding-standard": "7.4.0",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.5@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\PresenterMapping\\": "src/PresenterMapping/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
