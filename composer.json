{
    "name": "nepada/presenter-mapping",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Improved presenter mapping for PresenterFactory from Nette.",
    "keywords": ["nette", "presenter", "mapping"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/presenter-mapping/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nette/utils": "^3.0@dev",
        "nette/application": "^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "^2.3.2",
        "phpstan/phpstan": "^0.12.52",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "pepakriz/phpstan-exception-rules": "^0.11.1",
        "phpstan/phpstan-nette": "^0.12.6",
        "nepada/phpstan-nette-tester": "^0.3.0",
        "spaze/phpstan-disallowed-calls": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "nepada/coding-standard": "^7.2.1",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.1@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\PresenterMapping\\": "src/PresenterMapping/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
